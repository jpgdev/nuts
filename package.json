  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jpgdev/testhelper/issues"
  },
  "homepage": "https://github.com/jpgdev/testhelper#readme",
  "devDependencies": {
    "coveralls": "^2.11.12",
    "istanbul": "^0.4.4",
    "mocha": "^3.0.2",
    "should": "^11.1.0",
    "sinon": "^1.17.5"
  },
  "dependencies": {
    "async": "^2.0.1",
    "faker": "^3.0.1"
  }
}
